{
  "laxcomma" : true
  , "smarttabs" : true
  , "unused" : "vars"
  , "forin" : true
  , "quotmark" : "single"
  , "undef" : true
  , "browser" : true
  , "noempty" : true
  , "latedef" : true
  , "noarg" : true
  , "nonew" : true
  , "freeze" : true
  , "eqeqeq" : true
  , "immed" : true
  , "node" : true
  , "predef": [ "-Promise" ]
  , "globals" : {
    "define" : true
    , "require" : true
    , "requirejs" : true
    , "console" : true
    , "module" : true
    , "alert" : true
  }
}